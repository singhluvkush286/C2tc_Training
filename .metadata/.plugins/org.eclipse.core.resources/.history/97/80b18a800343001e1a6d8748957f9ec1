package com.tns.constructor;

import java.util.Scanner;

public class ConstructorDemo {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String name,city;
		int id;
		System.out.println("enter customer Id");
		id= sc.nextInt();
		sc.nextLine();
		System.out.println("enter customer Id");
		name= sc.next();
		System.out.println("enter customer Id");
		city= sc.next();

}
}
