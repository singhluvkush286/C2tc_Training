package com.tns.defaultConstructor;

import java.util.Scanner;

public class DefaultConstructor {
	public static void main() {
		String name;
		String address;
		int phone;
		String salesAmount;
		Scanner in = new Scanner(System.in);
		name=in.next();
		in.nextLine();
		address=in.next();
		in.nextLine();
		phone=in.nextInt();
		in.nextLine();
		
	}
}
